generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Notifications {
  EMAIL
  OFF
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  notif     Notifications @default(OFF)
  password  String
  createdAt DateTime @default(now())
}
